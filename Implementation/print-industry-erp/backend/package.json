{
  "name": "agogsaas-backend",
  "version": "1.0.0",
  "description": "AgogSaaS ERP Backend - GraphQL API (Portable Deployment)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev:legacy": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:legacy": "node dist/index.js",
    "test": "jest",
    "test:memory": "ts-node scripts/test-phase4-memory.ts",
    "test:nats": "ts-node scripts/test-nats-deliverables.ts",
    "test:orchestration": "ts-node scripts/test-orchestration.ts",
    "test:workflow": "ts-node scripts/test-end-to-end-workflow.ts",
    "smoke-test": "ts-node scripts/smoke-tests.ts",
    "workflow:item-master": "ts-node scripts/start-item-master-workflow.ts",
    "import:agog-memories": "ts-node scripts/import-agog-memories.ts",
    "init:nats-streams": "ts-node scripts/init-nats-streams.ts",
    "init:strategic-streams": "ts-node scripts/init-strategic-streams.ts",
    "host:listener": "ts-node scripts/host-agent-listener.ts",
    "daemon:start": "ts-node scripts/start-strategic-orchestrator.ts",
    "lint": "eslint src --ext .ts",
    "build:check": "tsc --noEmit",
    "type:check": "tsc --noEmit",
    "type:coverage": "npx type-coverage --at-least 95",
    "migrate": "echo 'Migrations applied via docker-entrypoint-initdb.d'",
    "k6:smoke": "k6 run k6/scenarios/smoke-test.js",
    "k6:load": "k6 run k6/scenarios/load-test.js",
    "k6:stress": "k6 run k6/scenarios/stress-test.js",
    "k6:spike": "k6 run k6/scenarios/spike-test.js",
    "k6:soak": "k6 run k6/scenarios/soak-test.js",
    "k6:db:connections": "k6 run k6/tests/database/connection-pool.js",
    "k6:db:query": "k6 run k6/tests/database/query-performance.js",
    "k6:db:write": "k6 run k6/tests/database/write-throughput.js",
    "k6:docker:up": "docker-compose -f k6/docker-compose.k6.yml up -d",
    "k6:docker:down": "docker-compose -f k6/docker-compose.k6.yml down",
    "k6:docker:smoke": "docker-compose -f k6/docker-compose.k6.yml run --rm k6 run /scenarios/smoke-test.js",
    "k6:docker:load": "docker-compose -f k6/docker-compose.k6.yml run --rm k6 run /scenarios/load-test.js",
    "k6:docker:stress": "docker-compose -f k6/docker-compose.k6.yml run --rm k6 run /scenarios/stress-test.js"
  },
  "keywords": [
    "graphql",
    "ai",
    "orchestration",
    "monitoring"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.3",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.51.1",
    "@opentelemetry/exporter-jaeger": "^1.28.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/instrumentation-express": "^0.43.0",
    "@opentelemetry/instrumentation-graphql": "^0.44.0",
    "@opentelemetry/instrumentation-http": "^0.56.1",
    "@opentelemetry/instrumentation-nestjs-core": "^0.41.0",
    "@opentelemetry/instrumentation-pg": "^0.48.0",
    "@opentelemetry/instrumentation-redis-4": "^0.43.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.56.1",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/stripe": "^8.0.416",
    "apollo-server": "^3.13.0",
    "apollo-server-plugin-base": "^3.7.2",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^5.0.1",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.12.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-subscriptions": "^3.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "nats": "^2.28.2",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "puppeteer": "^22.0.0",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "stripe": "^20.1.0",
    "twilio": "^5.11.1",
    "typeorm": "^0.3.28",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cache-manager": "^6.4.3",
    "cache-manager-redis-yet": "^5.1.5",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
