{
  "name": "agogsaas-backend",
  "version": "1.0.0",
  "description": "AgogSaaS Backend - GraphQL API with 4-layer AI system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
