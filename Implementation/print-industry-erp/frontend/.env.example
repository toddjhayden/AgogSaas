# AgogSaaS Frontend Environment Configuration
# Copy this file to .env and update with your values

# GraphQL API URL
# For docker-compose: Use 'backend' service name (internal) or 'localhost' (browser)
# Browser always connects to localhost, not docker service names
VITE_GRAPHQL_URL=http://localhost:4000/graphql

# Environment
VITE_APP_ENV=development

# Optional: Enable development tools
VITE_ENABLE_DEVTOOLS=true

# Optional: API timeout (milliseconds)
VITE_API_TIMEOUT=30000
